{
  "name": "eric-oss-app-ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npx playwright install && web-test-runner --playwright --browsers chromium",
    "test:all": "web-test-runner --playwright --browsers chromium firefox webkit",
    "test:watch": "web-test-runner --watch --playwright --browsers chromium",
    "start": "web-dev-server",
    "build:dev": "rollup -c",
    "build:prod": "rollup -c --environment BUILD:production",
    "srv": "node server",
    "nuke": "rm -rf node_modules && rm -rf build",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "format": "npm run format:eslint && npm run format:prettier",
    "lint:eslint": "eslint --ext .js,.html . --ignore-path .eslintignore",
    "format:eslint": "eslint --ext .js,.html . --fix --ignore-path .eslintignore",
    "lint:prettier": "prettier \"**/*.{js,html}\" --check --ignore-path .eslintignore",
    "format:prettier": "prettier \"**/*.{js,html}\" --write --ignore-path .eslintignore",
    "install:all": "npm install",
    "test:cypress": "npm i cypress && npx cypress run --spec cypress/e2e",
    "cypress:runDev": "npx cypress run --env configFile=developmentMocked",
    "cypress:runProd": "npx cypress run --env configFile=productionMocked",
    "cypress:openDev": "npx cypress open --env configFile=developmentMocked",
    "cypress:openProd": "npx cypress open --env configFile=productionMocked",
    "sonar-scanner:eric-oss-app-ui": "npx sonar-scanner"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "all"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@eui/app": "1.0.0",
    "@eui/base": "1.8.9",
    "@eui/component": "^1.0.4",
    "@eui/container": "1.2.3",
    "@eui/layout": "1.3.1",
    "@eui/lit-component": "1.2.0",
    "@eui/navigation-menu": "1.1.4",
    "@eui/table": "1.3.15",
    "@eui/theme": "^1.2.3",
    "@webcomponents/scoped-custom-element-registry": "^0.0.9",
    "es-module-shims": "1.9.0",
    "fs-extra": "^11.2.0",
    "lit-html": "3.1.3",
    "node-fetch": "^3.3.2",
    "urlpattern-polyfill": "10.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.24.4",
    "@babel/eslint-parser": "7.24.1",
    "@eui/rollup-config-generator": "1.2.1",
    "@open-wc/eslint-config": "12.0.3",
    "@open-wc/testing": "4.0.0",
    "@open-wc/testing-helpers": "3.0.0",
    "@shelex/cypress-allure-plugin": "^2.40.2",
    "@web/dev-server": "0.4.4",
    "@web/dev-server-import-maps": "0.2.0",
    "@web/dev-server-rollup": "0.6.1",
    "@web/test-runner": "0.18.1",
    "@web/test-runner-junit-reporter": "0.7.1",
    "@web/test-runner-playwright": "0.11.0",
    "cypress": "^13.10.0",
    "eslint": "8.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-lit-a11y": "4.1.2",
    "express": "4.19.2",
    "prettier": "3.2.5",
    "rollup-plugin-string": "3.0.0",
    "sinon": "^17.0.1"
  }
}
